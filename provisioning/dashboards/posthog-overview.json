{
  "uid": "posthog-overview",
  "title": "PostHog Overview",
  "description": "Overview of PostHog event data including event counts, trends, top events, and page analytics.",
  "tags": ["posthog"],
  "timezone": "utc",
  "editable": true,
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "refresh": "5m",
  "panels": [
    {
      "id": 1,
      "title": "Total Events",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": { "type": "chartcastr-posthog-datasource", "uid": "posthog" },
      "targets": [
        {
          "refId": "A",
          "queryType": "hogql",
          "rawHogQL": "SELECT count() AS total_events FROM events WHERE timestamp >= $__timeFrom AND timestamp < $__timeTo"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto"
      }
    },
    {
      "id": 2,
      "title": "Unique Persons",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": { "type": "chartcastr-posthog-datasource", "uid": "posthog" },
      "targets": [
        {
          "refId": "A",
          "queryType": "hogql",
          "rawHogQL": "SELECT count(DISTINCT distinct_id) AS unique_persons FROM events WHERE timestamp >= $__timeFrom AND timestamp < $__timeTo"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto"
      }
    },
    {
      "id": 3,
      "title": "Avg Events / Day",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "datasource": { "type": "chartcastr-posthog-datasource", "uid": "posthog" },
      "targets": [
        {
          "refId": "A",
          "queryType": "hogql",
          "rawHogQL": "SELECT round(count() / greatest(dateDiff('day', toDateTime($__timeFrom), toDateTime($__timeTo)), 1)) AS avg_per_day FROM events WHERE timestamp >= $__timeFrom AND timestamp < $__timeTo"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "orange", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto"
      }
    },
    {
      "id": 4,
      "title": "Distinct Event Types",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "datasource": { "type": "chartcastr-posthog-datasource", "uid": "posthog" },
      "targets": [
        {
          "refId": "A",
          "queryType": "hogql",
          "rawHogQL": "SELECT count(DISTINCT event) AS event_types FROM events WHERE timestamp >= $__timeFrom AND timestamp < $__timeTo"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "purple", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "auto"
      }
    },
    {
      "id": 5,
      "title": "Events Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 4 },
      "datasource": { "type": "chartcastr-posthog-datasource", "uid": "posthog" },
      "targets": [
        {
          "refId": "A",
          "queryType": "hogql",
          "rawHogQL": "SELECT toStartOfDay(timestamp) AS time, count() AS event_count FROM events WHERE timestamp >= $__timeFrom AND timestamp < $__timeTo GROUP BY time ORDER BY time"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 6,
      "title": "Top Events",
      "type": "barchart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "chartcastr-posthog-datasource", "uid": "posthog" },
      "targets": [
        {
          "refId": "A",
          "queryType": "hogql",
          "rawHogQL": "SELECT event, count() AS event_count FROM events WHERE timestamp >= $__timeFrom AND timestamp < $__timeTo GROUP BY event ORDER BY event_count DESC LIMIT 15"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "xTickLabelRotation": 0,
        "legend": { "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 7,
      "title": "Events by Browser",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "chartcastr-posthog-datasource", "uid": "posthog" },
      "targets": [
        {
          "refId": "A",
          "queryType": "hogql",
          "rawHogQL": "SELECT coalesce(properties.$browser, 'Unknown') AS browser, count() AS event_count FROM events WHERE timestamp >= $__timeFrom AND timestamp < $__timeTo GROUP BY browser ORDER BY event_count DESC LIMIT 10"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "pieType": "pie",
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }
      }
    },
    {
      "id": 8,
      "title": "Top Pages",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 20 },
      "datasource": { "type": "chartcastr-posthog-datasource", "uid": "posthog" },
      "targets": [
        {
          "refId": "A",
          "queryType": "hogql",
          "rawHogQL": "SELECT properties.$current_url AS url, count() AS pageviews, count(DISTINCT distinct_id) AS unique_visitors FROM events WHERE event = '$pageview' AND timestamp >= $__timeFrom AND timestamp < $__timeTo GROUP BY url ORDER BY pageviews DESC LIMIT 20"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "pageviews", "desc": true }]
      }
    }
  ],
  "schemaVersion": 39,
  "version": 1
}
